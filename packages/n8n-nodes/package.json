{
  "name": "n8n-nodes-nocioun",
  "version": "1.0.0",
  "description": "n8n nodes for Google Contacts and Notion integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run clean && npm run build"
  },
  "files": [
    "dist/",
    "nodes/",
    "credentials/",
    "README.md"
  ],
  "keywords": [
    "n8n",
    "n8n-nodes",
    "google-contacts",
    "notion",
    "integration",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaonmir/nocioun.git",
    "directory": "packages/n8n-nodes"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GoogleContactsApi.credentials.js",
      "dist/credentials/NotionApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GoogleContacts/GoogleContacts.node.js",
      "dist/nodes/Notion/Notion.node.js"
    ]
  },
  "dependencies": {
    "@nocioun/core": "workspace:*",
    "@notionhq/client": "^3.1.3",
    "google-auth-library": "^10.1.0",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
